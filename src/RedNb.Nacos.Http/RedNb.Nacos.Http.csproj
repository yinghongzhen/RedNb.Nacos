<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>preview</LangVersion>
    <RootNamespace>RedNb.Nacos.Http</RootNamespace>
    <AssemblyName>RedNb.Nacos.Http</AssemblyName>
    <Version>1.0.0</Version>
    <Authors>RedNb</Authors>
    <Description>Nacos .NET SDK HTTP Client - HTTP-based Nacos client implementation</Description>
    <PackageTags>nacos;service-discovery;config;microservices;http</PackageTags>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageId>RedNb.Nacos.Http</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\RedNb.Nacos\RedNb.Nacos.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Http" Version="9.0.0" />
    <PackageReference Include="Polly" Version="8.5.1" />
    <PackageReference Include="Polly.Extensions" Version="8.5.1" />
  </ItemGroup>

</Project>
